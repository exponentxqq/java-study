<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.database.mybatis.mapper.StudentMapper">
  <select id="select" resultType="com.study.database.mybatis.domain.Student">
    select *
    from students
    where id = 1
  </select>
</mapper>